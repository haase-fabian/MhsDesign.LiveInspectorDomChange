{
    "description": "Neos Live Inspector Package, which reflects changes to the dom via MhsDesign.LiveInspectorJsEvents",
    "type": "neos-package",
    "name": "mhsdesign/liveinspectordomchange",
    "license": "GPL-3.0+",
    "require": {
        "neos/neos": "^5.0 || ^7.0 || ^8.0",
        "neos/flow": "^6.0 || ^7.0 || ^8.0",
        "mhsdesign/liveinspectorjsevents": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MhsDesign\\LiveInspectorDomChange\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MhsDesign\\LiveInspectorDomChange\\Tests\\": "Tests"
        }
    },
    "extra": {
        "neos": {
            "package-key": "MhsDesign.LiveInspectorDomChange"
        }
    }
}
